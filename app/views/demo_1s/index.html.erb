<h1>Listing demo_1s</h1>

<table>
  <thead>
    <tr>
      <th>New demo</th>
      <th>New demo 1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @demo_1s.each do |demo_1| %>
      <tr>
        <td><%= demo_1.new_demo %></td>
        <td><%= demo_1.new_demo_1 %></td>
        <td><%= link_to 'Show', demo_1 %></td>
        <td><%= link_to 'Edit', edit_demo_1_path(demo_1) %></td>
        <td><%= link_to 'Destroy', demo_1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

